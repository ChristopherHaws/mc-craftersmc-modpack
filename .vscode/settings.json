{
	"json.schemas": [
		{
			"fileMatch": [
				"*.pw.toml"
			],
			"url": "../.schemas/packwiz-mod.schema.json"
		},
		{
			"fileMatch": [
				"/lang/*.json"
			],
			"url": "./.vscode/probe.lang-schema.json"
		},
		{
			"fileMatch": [
				"/probe/docs/*.json"
			],
			"url": "./.vscode/probe.doc-schema.json"
		}
	],
	"files.associations": {
		"**/dynmap/**/*.txt": "yaml",
		".packwizignore": "ignore"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"evenBetterToml.schema.associations": {
		"^.*\\.pw\\.toml$": "file:///C:/dev/chaws/mc/chaws-modpack/.schemas/packwiz-mod.schema.json"
	},
