{
	/* ***********************
	   Backpack Safety
	   - Does not allow opening a
	   backpack if more than one is
	   on the hotbar
	   - This prevents posibble dup'ing
	   ***********************
	*/
	"enableSingleBackpackOnHotbar": true,
	/* ***********************
	   Backpack Safety
	   - Does not allow opening a
	   backpack in the mainhand
	   - This prevents posibble dup'ing
	   ***********************
	*/
	"requireOffhandOpening": true
}