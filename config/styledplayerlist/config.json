{
  "CONFIG_VERSION_DONT_TOUCH_THIS": 1,
  "_comment": "Before changing anything, see https://github.com/Patbox/StyledPlayerList#configuration",
  "defaultStyle": "default",
  "updateRate": 20,
  "displayOnSingleplayer": true,
  "switchMessage": "Your player list style has been changed to: <gold>${style}</gold>",
  "unknownStyleMessage": "<red>This style doesn't exist!</red>",
  "permissionMessage": "<red>You don't have required permissions!</red>",
  "changePlayerName": true,
  "playerNameFormat": "%styled-nicknames:display_name%",
  "updatePlayerNameEveryChatMessage": false,
  "playerNameUpdateRate": 100,
  "permissionNameFormat": [
    {
      "permission": "group.admin",
      "opLevel": -1,
      "style": "<gold>Admin</gold> %styled-nicknames:display_name%"
    },
    {
      "permission": "group.mod",
      "opLevel": -1,
      "style": "<red>Mod</red> %styled-nicknames:display_name%"
    },
    {
      "permission": "group.citizen",
      "opLevel": -1,
      "style": "<green>Citizen</green> %styled-nicknames:display_name%"
    },
    {
      "permission": "group.default",
      "opLevel": -1,
      "style": "<gray>Visitor</gray> %styled-nicknames:display_name%"
    }
  ]
}